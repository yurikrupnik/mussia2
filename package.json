{
  "name": "mussia2",
  "version": "1.0.0",
  "description": "fullstack",
  "main": "src/server.jsx",
  "scripts": {
    "start": "npm run start:client & npm run start:server",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start:server": "webpack -w --config webpack.config.server.js",
    "start:mongo": "docker run --rm -d -p 27017:27017 --name mongo mongo",
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run build:server & npm run build:client",
    "build:server": "webpack --env.prod --config webpack.config.server.js",
    "build:client": "webpack --env.prod --config webpack.config.client.js",
    "lint": "eslint --ext js,jsx src",
    "test": "jest src/",
    "test:watch": "jest src/ --watch",
    "test:coverage": "jest src/ --coverage",
    "test:e2e": "jest e2e/",
    "clean:images": "rimraf e2e/screens/",
    "deploy:test": "npm run lint && npm run test",
    "deploy:build": "npm run deploy:test && npm run build && docker build -t boil-local . ",
    "deploy:run": "docker run -p 7000:7000 boil-local",
    "deploy:compose": "npm run deploy:test && npm run build && docker-compose up",
    "deploy:up": "docker run -p 7000:7000 boil",
    "debug:server": "webpack --env.debug --config webpack.config.server.js -w",
    "debug:client": "webpack --env.debug --config webpack.config.client.js -w",
    "watch:client": "webpack --config webpack.config.client.js -w",
    "debug": "npm run debug:server & npm run debug:client",
    "dist": "npm run build && cd dist && npm start",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build:storybook": "build-storybook -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/mussia2.git"
  },
  "author": {
    "name": "Yuri Krupnik",
    "email": "krupnik.yuri@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/mussia2/issues"
  },
  "homepage": "https://github.com/yurikrupnik/mussia2#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "device-detector": "^1.0.2",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "flexboxgrid": "^6.3.1",
    "lodash": "^4.17.11",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.0.3",
    "mongoose": "^5.4.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-json-view": "^1.19.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-render-html": "^0.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
